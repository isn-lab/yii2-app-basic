{
  "name": "isn-lab/yii2-app-basic",
  "description": "Yii 2 Basic Project Template for ISN",
  "keywords": [
    "yii2",
    "framework",
    "basic",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.5",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "isnlab/yii2-isn-common": "dev-master",
    "isnlab/yii2-isn-auth": "dev-master"
  },
  "require-dev": {
    "codeception/codeception" : ">=2.0.4",
    "codeception/specify" : "*",
    "codeception/verify" : "*",
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800,
    "secure-http": false
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "isnlab\\common\\composer\\Installer::postCreateProject"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755",
          "isn_install": "0755",
          "isn_update": "0755",
          "isn_repo": "0755"
        }
      ],
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://svn.isnlab.com/scm/svn/isnlab_packages/yii2/common" },
    { "type": "vcs", "url": "https://svn.isnlab.com/scm/svn/isnlab_packages/yii2/auth" }
  ]
}
